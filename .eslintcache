unt":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"21"},{"filePath":"35","messages":"36","errorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":"37","usedDeprecatedRules":"21"},{"filePath":"38","messages":"39","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/home/alex-desktop/work/personalsite/atenczar/src/index.js",["40","41","42"],"import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\nimport SimpleReactLightbox from 'simple-react-lightbox'\nimport {BrowserRouter as Router, Switch, Route, HashRouter} from 'react-router-dom';\n\nReactDOM.render(\n  <React.StrictMode>\n    <HashRouter>    \n      <SimpleReactLightbox>\n        <App />\n      </SimpleReactLightbox>\n    </HashRouter>\n  </React.StrictMode>,\n  document.getElementById('root')\n);",["43","44"],"/home/alex-desktop/work/personalsite/atenczar/src/App.js",["45"],"import React, { Component } from 'react';\nimport './scss/App.scss';\nimport TopNav from './components/TopNav.js';\nimport BottomNav from './components/BottomNav.js';\nimport { Switch, Route} from 'react-router-dom';\nimport Home from './components/pages/Home.js';\nimport About from './components/pages/About.js';\nimport Projects from './components/pages/Projects.js';\nimport Contact from './components/pages/Contact.js';\n\nclass App extends Component{\n\n  constructor(props) {\n    super(props);\n    this.state = {hover: \"\"};}\n\n    changeHover = hover => {\n      this.setState({\n        hover\n      });\n    };\n\n    returnThemeBasedOnTime(){\n      const root = document.documentElement\n      let date = new Date\n      const hour = date.getHours()\n      if(this.returnThemeBasedOnOS === undefined){\n        if (hour > 20 || hour < 5) {\n          root.classList.add('dark')\n          root.classList.remove('light')\n        }\n        else {\n          root.classList.add('light')\n          root.classList.remove('dark')\n        }\n      } \n    }\n\n    returnThemeBasedOnOS() {\n      const root = document.documentElement\n      let pref = window.matchMedia('(prefers-color-scheme: dark)')\n      if (pref.matches) root.classList.add('dark')\n      else {\n        pref = window.matchMedia('(prefers-color-scheme: light)')\n        if (pref.matches) root.classList.add('light')\n        else return undefined\n      }\n    }\n    \n    componentDidMount() {\n      this.returnThemeBasedOnOS();\n      this.returnThemeBasedOnTime();\n    }\n\n  render() {\n    return (\n      <div className=\"wrapper\">\n        <header>\n          <TopNav />\n        </header>\n        <div id=\"content\" className=\"content-container\">\n        <Switch>\n            <Route path='/' exact render={(props) => (\n            <Home {...props} hover={this.state.hover} />)}/>\n            <Route path='/about' exact component={About}/>\n            <Route path='/projects' exact component={Projects} />\n            <Route path='/contact' exact component={Contact} />\n          </Switch>\n        </div>\n        <footer>\n          <BottomNav hoverValue={this.changeHover}/>\n        </footer>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/home/alex-desktop/work/personalsite/atenczar/src/components/TopNav.js",["46"],"import '../scss/nav.scss';\nimport '../scss/nav.scss';\nimport React, {useState} from 'react'\nimport { Link } from 'react-router-dom';\n\nfunction TopNav() {\n  return (\n    <>\n      <nav className=\"nav\">\n        <div className=\"nav-top\">\n          <Link to=\"/\" className=\"nav-item\">alex tenczar</Link>\n        </div>\n      </nav>\n    </>\n  )\n}\n\nexport default TopNav\n","/home/alex-desktop/work/personalsite/atenczar/src/components/BottomNav.js",["47"],"import '../scss/nav.scss';\nimport React, {useState, Component} from 'react'\nimport { NavLink } from 'react-router-dom';\n\nclass BottomNav extends Component {\n  update = (val) => {\n    this.props.hoverValue(val);\n  }\n  render () {\n      return (\n      <>\n        <nav className=\"nav\">\n          <div className=\"nav-bottom\">\n            <NavLink to=\"/about\" className=\"nav-item\" activeClassName=\"selected\" onMouseEnter={() => this.update('who am i?')} onMouseLeave={() => this.update('')}>about</NavLink>\n            <NavLink to=\"/projects\" className=\"nav-item\" activeClassName=\"selected\"onMouseEnter={() => this.update(\"things i've worked on.\")} onMouseLeave={() => this.update('')}>projects</NavLink>\n            <NavLink to=\"/contact\" className=\"nav-item\" activeClassName=\"selected\"onMouseEnter={() => this.update('get in touch with me.')} onMouseLeave={() => this.update('')}>contact</NavLink>\n          </div>\n        </nav>\n      </>\n      );\n  }\n}\nexport default BottomNav;\n","/home/alex-desktop/work/personalsite/atenczar/src/components/pages/Home.js",[],"/home/alex-desktop/work/personalsite/atenczar/src/components/pages/About.js",[],"/home/alex-desktop/work/personalsite/atenczar/src/components/pages/Projects.js",["48","49"],"import '../../scss/App.scss';\nimport '../../scss/projects.scss';\nimport { SRLWrapper } from \"simple-react-lightbox\";\nimport djangoproject from '../../static/django-project.png';\nimport cpproject from '../../static/cp-project.png';\n\nconst options = {\n    buttons: {\n        showDownloadButton: false,\n        showAutoplayButton: false,\n        showFullscreenButton: false,\n        showThumbnailsButton: false,\n        showNextButton: false,\n        showPrevButton: false,\n    },\n    thumbnails: {\n        showThumbnails: false,\n    }\n};\n\nfunction Projects () {\n    return (\n    <SRLWrapper options={options}>\n        <div id=\"Projects\">\n            <div className=\"project\">\n                <div className=\"project-text\">\n                    <a href=\"https://github.com/alextenczar/3-julian-alex\" className=\"project-link\" target=\"_blank\" rel=\"noopener noreferrer\">Django Spreadsheet Web App</a>\n                    <p>A fellow classmate and I created a Django web app capable of importing a Microsoft Excel spreadsheet and populating various data models within a PostGreSQL database. The web app could then display the models and sort them by any attribute. The web app and database were eventually deployed to <a href=\"https://nhsee-team3.herokuapp.com/\" target=\"_blank\" rel=\"noopener noreferrer\">Heroku</a> for remote access.</p>\n                    <p><strong>Technologies used:</strong> Django, Python, PostGreSQL, Heroku</p>\n                </div>\n                <div className=\"project-image\">\n                    <img src={djangoproject}></img>\n                </div>\n            </div>\n            <div className=\"project\">\n                <div className=\"project-text\">\n                    <h2>React Front End for Drupal 8</h2>\n                    <p>During my internship at CommonPlaces Interactive, I built a React front end on top of their existing Drupal workflow with Create-React-App and GraphQL.</p>\n                    <p><strong>Technologies used: React.js, Drupal 8, GraphQL, Docker, Storybook</strong></p>\n                </div>\n                <div className=\"project-image\">\n                    <img src={cpproject}></img>\n                </div>\n            </div>\n        </div>\n        </SRLWrapper>\n    )\n};\n\nexport default Projects;","/home/alex-desktop/work/personalsite/atenczar/src/components/pages/Contact.js",[],{"ruleId":"50","severity":1,"message":"51","line":5,"column":26,"nodeType":"52","messageId":"53","endLine":5,"endColumn":32},{"ruleId":"50","severity":1,"message":"54","line":5,"column":34,"nodeType":"52","messageId":"53","endLine":5,"endColumn":40},{"ruleId":"50","severity":1,"message":"55","line":5,"column":42,"nodeType":"52","messageId":"53","endLine":5,"endColumn":47},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":25,"column":18,"nodeType":