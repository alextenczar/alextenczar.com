remove('dark')\n        }\n      } \n    }\n\n    returnThemeBasedOnOS() {\n      const root = document.documentElement\n      let pref = window.matchMedia('(prefers-color-scheme: dark)')\n      if (pref.matches) root.classList.add('dark')\n      else {\n        pref = window.matchMedia('(prefers-color-scheme: light)')\n        if (pref.matches) root.classList.add('light')\n        else return undefined\n      }\n    }\n    \n    componentDidMount() {\n      this.returnThemeBasedOnOS();\n      this.returnThemeBasedOnTime();\n    }\n\n  render() {\n    return (\n      <div className=\"wrapper\">\n        <header>\n          <TopNav />\n        </header>\n        <div id=\"content\" className=\"content-container\">\n        <Switch>\n            <Route path='/' exact render={(props) => (\n            <Home {...props} hover={this.state.hover} />)}/>\n            <Route path='/about' exact component={About}/>\n            <Route path='/projects' exact component={Projects} />\n            <Route path='/contact' exact component={Contact} />\n          </Switch>\n        </div>\n        <footer>\n          <BottomNav hoverValue={this.changeHover}/>\n        </footer>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/home/alex-desktop/work/personalsite/atenczar/src/components/TopNav.js",["46"],"import '../scss/nav.scss';\nimport '../scss/nav.scss';\nimport React, {useState} from 'react'\nimport { Link } from 'react-router-dom';\n\nfunction TopNav() {\n  return (\n    <>\n      <nav className=\"nav\">\n        <div className=\"nav-top\">\n          <Link to=\"/\" className=\"nav-item\">alex tenczar</Link>\n        </div>\n      </nav>\n    </>\n  )\n}\n\nexport default TopNav\n","/home/alex-desktop/work/personalsite/atenczar/src/components/BottomNav.js",["47"],"import '../scss/nav.scss';\nimport React, {useState, Component} from 'react'\nimport { NavLink } from 'react-router-dom';\n\nclass BottomNav extends Component {\n  update = (val) => {\n    this.props.hoverValue(val);\n  }\n  render () {\n      return (\n      <>\n        <nav className=\"nav\">\n          <div className=\"nav-bottom\">\n            <NavLink to=\"/about\" className=\"nav-item\" activeClassName=\"selected\" onMouseEnter={() => this.update('who am i?')} onMouseLeave={() => this.update('')}>about</NavLink>\n            <NavLink to=\"/projects\" className=\"nav-item\" activeClassName=\"selected\"onMouseEnter={() => this.update(\"things i've worked on.\")} onMouseLeave={() => this.update('')}>projects</NavLink>\n            <NavLink to=\"/contact\" className=\"nav-item\" activeClassName=\"selected\"onMouseEnter={() => this.update('get in touch with me.')} onMouseLeave={() => this.update('')}>contact</NavLink>\n          </div>\n        </nav>\n      </>\n      );\n  }\n}\nexport default BottomNav;\n","/home/alex-desktop/work/personalsite/atenczar/src/components/pages/Home.js",[],"/home/alex-desktop/work/personalsite/atenczar/src/components/pages/About.js",["48"],"import '../../scss/App.scss';\nimport Clock from 'react-live-clock';\nimport '../../scss/about.scss'\n\nfunction About () {\n    return (\n        <div id=\"About\">\n            <p>üìç Concord, NH, USA:</p><Clock format={'LTS'} ticking={true} timezone={'America/New_York'} />\n            <p id=\"bio\">Hi. I'm an Information Technology graduate student at the <a href=\"https://manchester.unh.edu/\">University of New Hampshire at Manchester</a>. Over the past few years I've become especially interested in pursuing web development. I enjoy using Python and React to make my ideas come to life. I'm currently working as a freelance front-end web developer while I complete my final year of school.</p>\n            <a id=\"resume\">resume</a>\n        </div>\n    )\n};\n\nexport default About;","/home/alex-desktop/work/personalsite/atenczar/src/components/pages/Projects.js",["49","50"],"/home/alex-desktop/work/personalsite/atenczar/src/components/pages/Contact.js",[],{"ruleId":"51","severity":1,"message":"5